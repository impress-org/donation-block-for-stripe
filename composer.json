{
    "name": "givewp/donation-block-for-stripe",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Jason Adams",
            "email": "jason.adams@givewp.com"
        },
        {
            "name": "Devin Walker",
            "email": "devin@givewp.com"
        }
    ],
    "require": {
        "stripe/stripe-php": "^7.113"
    },
    "autoload": {
        "psr-4": {
            "GiveDonationBlock\\": "src/"
        }
    },
    "require-dev": {
        "brianhenryie/strauss": "^0.11.0"
    },
    "scripts": {
        "strauss": [
            "vendor/bin/strauss"
        ],
        "post-install-cmd": [
            "@strauss"
        ],
        "post-update-cmd": [
            "@strauss"
        ]
    },
    "extra": {
        "strauss": {
            "target_directory": "src/Strauss",
            "namespace_prefix": "GiveDonationBlock\\Strauss\\",
            "classmap_prefix": "GiveDonationBlock_Strauss_",
            "constant_prefix": "DONATION_BLOCK_",
            "packages": [
            ],
            "override_autoload": {
                "stripe/stripe-php": {
                    "psr-4": {
                        "Stripe\\": "lib/"
                    },
                    "files": [
                        "data"
                    ]
                }
            },
            "exclude_from_copy": {
                "packages": [
                ],
                "namespaces": [
                ],
                "file_patterns": [
                ]
            },
            "exclude_from_prefix": {
                "packages": [
                ],
                "namespaces": [
                ],
                "file_patterns": [
                    "/^psr.*$/"
                ]
            },
            "namespace_replacement_patterns": {},
            "delete_vendor_files": false
        }
    }
}
